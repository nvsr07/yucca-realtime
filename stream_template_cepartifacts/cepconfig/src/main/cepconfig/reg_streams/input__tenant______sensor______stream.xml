<@pp.dropOutputFile />
<@pp.changeOutputFile name="input__"+tenant.code+"__"+sensor.code_+"_"+stream.code+".xml" />
{
  "name": "input__${tenant.code}__${sensor.code_}__${stream.code}",
  "version": "1.0.0",
  "nickName": "input__${tenant.code}__${sensor.code_}__${stream.code}",
  "description": "Stream input__${tenant.code}__${sensor.code_}__${stream.code}",
  "metaData": [
    {
      "name": "Sensor",
      "type": "STRING"
    }
  ],
  "payloadData": [
    {
      "name": "time",
      "type": "STRING"
    },
    <#list components as component>
     { 
      "name": "${component.code}",
      "type": "${component.type}"
     }<#if component_has_next>,</#if>
     </#list>
  ]
}