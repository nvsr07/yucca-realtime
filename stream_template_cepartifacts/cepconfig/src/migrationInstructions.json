# Migrazione alla 1.4.0 , rimozione stat queue  

<#assign x = 1> 
{
"steps": [
<#list mbs as mb>
{
"number": "${x}",<#assign x = x+1> 
"description":"Rimozione event Formatter per ogni nodo CEP : api__deploy__output__${tenant.code}__${source.code_}_${stream.code}__stat__EventFormatter", 
"actionType":"WebServiceCall",
"sourceType":"file",
"source":"cepconfig/eventformatters/api__delete__output__${tenant.code}__${source.code_}_${stream.code}__stat__EventFormatter.xml",
"action":"undeployActiveEventFormatterConfiguration",
"destination":"${mb.host}/services/EventFormatterAdminService",
"returnExpected":"200", 
"validityControl":"NO_CHECK",
"username":"${mb.secuser}", 
"password":"${mb.secpassword}"
},{
"number": "${x}",<#assign x = x+1> 
"description":"Installazione event Execution Plans per ogni nodo CEP : api__update__process__${tenant.code}__${source.code_}_${stream.code}__ExecutionPlan", 
"actionType":"WebServiceCall",
"sourceType":"file",
"source":"cepconfig/executionplans/api__update__process__${tenant.code}__${source.code_}_${stream.code}__ExecutionPlan.xml",
"action":"editActiveExecutionPlanConfiguration",
"destination":"${mb.host}/services/EventProcessorAdminService",
"returnExpected":"200", 
"validityControl":"<fault>",
"username":"${mb.secuser}", 
"password":"${mb.secpassword}"
},{
"number": "${x}",<#assign x = x+1> 
"description":"Rimozione degli Stream in un nodo CEP", 
"actionType":"WebServiceCall",
"sourceType":"file",
"source":"cepconfig/reg_streams/api__delete__output__${tenant.code}__${source.code_}_${stream.code}__stat.xml",
"action":"removeEventStreamInfo",
"destination":"${mb.host}/services/EventStreamAdminService",
"returnExpected":"200", 
"validityControl":"NO_CHECK",
"username":"${mb.secuser}", 
"password":"${mb.secpassword}"
}
<#if mb_has_next>,</#if>
</#list>
]
}